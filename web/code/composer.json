{
    "name": "example/web",
    "description": "Example web application",
    "config": {
       "platform": {
           "php": "7.4.1"
       }
    },
    "require": {
        "php": ">=7.4.1",
        "nikic/fast-route": "1.3",
        "filp/whoops": "2.7.2",
        "monolog/monolog": "2.0.2",
        "symfony/dependency-injection": "5.0.8",
        "symfony/config": "5.0.8",
        "php-di/invoker": "2.0.0",
        "nesbot/carbon": "2.33.0",
        "symfony/http-foundation": "5.0.8",
        "symfony/error-handler": "5.0.8",
        "twig/twig": "3.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "9.1.4",
        "mockery/mockery": "1.3.1",
        "fzaninotto/faker": "1.9.1"
    },
    "autoload": {
        "psr-4": {
            "Example\\": "src/",
            "Example\\Tests\\": "tests/",
            "Mini\\": "mini/"
        },
        "files": [
            "mini/helpers.php"
        ]
    }
}
